          <column name="organization_id" type="BIGINT">
              <constraints nullable="false"/>
          </column>
      </createTable>

        <addForeignKeyConstraint baseColumnNames="event_id"
                                 baseTableName="product"
                                 constraintName="fk_product_event"
                                 onDelete="CASCADE"
                                 onUpdate="RESTRICT"
                                 referencedColumnNames="id"
